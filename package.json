{
  "name": "omw",
  "version": "0.3.0",
  "repository": "https://github.com/mcdafydd/omw-pwa",
  "description": "A minimalist time tracker inspired by the Ultimate Time Tracker",
  "main": "omw-app.js",
  "keywords": [
    "Omw",
    "Time tracking"
  ],
  "author": "mcdafydd",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@fullcalendar/core": "^4.4.0",
    "@fullcalendar/daygrid": "^4.4.0",
    "@fullcalendar/interaction": "^4.4.0",
    "@fullcalendar/moment-timezone": "^4.4.0",
    "@fullcalendar/timegrid": "^4.4.0",
    "@material/mwc-base": "^0.14.1",
    "@material/mwc-button": "^0.14.1",
    "@material/mwc-dialog": "^0.14.1",
    "@material/mwc-drawer": "^0.14.1",
    "@material/mwc-fab": "^0.14.1",
    "@material/mwc-icon": "^0.14.1",
    "@material/mwc-icon-button": "^0.14.1",
    "@material/mwc-list": "^0.14.1",
    "@material/mwc-menu": "^0.14.1",
    "@material/mwc-ripple": "^0.14.1",
    "@material/mwc-snackbar": "^0.14.1",
    "@material/mwc-textarea": "^0.14.1",
    "@material/mwc-textfield": "^0.14.1",
    "@material/mwc-top-app-bar-fixed": "^0.14.1",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/polymer": "^3.3.0",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "dark-mode-toggle": "^0.5.1",
    "dexie": "^3.0.0-rc.6",
    "dexie-export-import": "^1.0.0-rc.1",
    "dexie-observable": "^1.0.0-beta.6",
    "downloadjs": "^1.4.7",
    "lit-element": "^2.3.1",
    "lit-element-router": "2.0.0-rc.16",
    "lit-html": "^1.2.1",
    "markty-toml": "^0.1.1",
    "material-components-web": "^5.1.0",
    "moment-timezone": "^0.5.28",
    "regenerator-runtime": "^0.13.5",
    "systemjs": "^6.2.6",
    "tslib": "^1.11.1",
    "uuid": "^7.0.2",
    "weightless": "0.0.37"
  },
  "scripts": {
    "prepare": "snowpack",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:compatibility": "karma start --compatibility all --coverage",
    "test:compatibility:watch": "karma start --compatibility all --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --coverage",
    "type-check": "tsc",
    "start:dev": "es-dev-server --open",
    "start:watch": "npx babel src/ --out-dir lib --extensions .js --extensions .ts --watch && es-dev-server --open --watch",
    "build": "npm run build:babel && npm run build:sass && npx snowpack && npm run build:sw",
    "build:babel": "npx babel src/ --out-dir lib --extensions .js --extensions .ts",
    "build:release": "export NODE_ENV=production && rimraf dist && mkdir dist && npm run build:babel && npm run build:sass && npx snowpack --dest dist/web_modules --optimize && npm run build:sw && cp -a css fonts images lib index.html manifest.json sw.js dist/",
    "build:sass": "npx gulp sass",
    "build:sw": "npx gulp generate-sw"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/parser": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@open-wc/eslint-config": "^2.0.4",
    "@open-wc/prettier-config": "^0.1.14",
    "@open-wc/testing": "^2.5.8",
    "@open-wc/testing-karma": "^3.3.9",
    "@open-wc/testing-karma-bs": "^1.3.53",
    "@polymer/test-fixture": "^4.0.2",
    "autoprefixer": "^9.7.5",
    "axe-core": "^3.5.2",
    "chai": "^4.1.2",
    "command-line-args": "^5.1.1",
    "del": "^3.0.0",
    "es-dev-server": "^1.45.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.0.2",
    "husky": "^1.0.0",
    "jshint": "^2.11.0",
    "lint-staged": "^8.0.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.13.1",
    "pixelmatch": "^4.0.2",
    "postcss": "^7.0.27",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "rimraf": "^2.7.1",
    "snowpack": "^1.6.0",
    "typescript": "^3.8.3",
    "wct-mocha": "^1.0.0",
    "webpack-merge": "^4.1.5",
    "workbox-build": "^5.1.2"
  },
  "snowpack": {
    "webDependencies": [
      "@fullcalendar/core",
      "@fullcalendar/daygrid",
      "@fullcalendar/interaction",
      "@fullcalendar/moment-timezone",
      "@fullcalendar/timegrid",
      "@material/mwc-base/base-element.js",
      "@material/mwc-button",
      "@material/mwc-dialog",
      "@material/mwc-drawer",
      "@material/mwc-fab",
      "@material/mwc-icon",
      "@material/mwc-icon-button",
      "@material/mwc-list/mwc-list-item.js",
      "@material/mwc-list",
      "@material/mwc-menu",
      "@material/mwc-ripple",
      "@material/mwc-snackbar",
      "@material/mwc-textarea",
      "@material/mwc-textfield",
      "@material/mwc-top-app-bar-fixed",
      "dark-mode-toggle",
      "dexie",
      "dexie-export-import",
      "dexie-observable",
      "downloadjs",
      "lit-element",
      "lit-element-router",
      "lit-html",
      "lit-html/directives/if-defined.js",
      "markty-toml",
      "tslib",
      "uuid",
      "weightless"
    ],
    "dedupe": [
      "@fullcalendar/core",
      "@fullcalendar/daygrid",
      "@fullcalendar/interaction",
      "@fullcalendar/moment-timezone",
      "@fullcalendar/timegrid",
      "@material/mwc-base/base-element.js",
      "@material/mwc-dialog",
      "lit-element",
      "lit-html",
      "tslib"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    }
  },
  "prettier": "@open-wc/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:compatibility": "karma start --compatibility all --coverage",
    "test:compatibility:watch": "karma start --compatibility all --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --compatibility all --coverage",
    "start:build": "cd dist && es-dev-server --open"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
